<%= form_for(@reservation_frame) do |f| %>
  <div class="mx-auto mt-5">
    <%= render 'shared/error_form', model: f.object %>
    <div class="mb-3">
      <%= f.date_field :date %>
    </div>
    <div class="mb-3">
      <%= f.collection_select :time_frame_id, TimeFrame.all, :id, :start_at, {prompt: "開始時間を選択"} %>
    </div>
    <div class="mb-3">
      <%= f.submit '登録', class: "btn btn-primary" %>
    </div>
  </div>
<% end %>
